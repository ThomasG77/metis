ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name mapd-data-layer
src_root_dir = /Users/Tai/workspace/mapd-data-layer
bsc = /Users/Tai/workspace/mapd-data-layer/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/Tai/workspace/mapd-data-layer/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I '/Users/Tai/workspace/mapd-data-layer/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /Users/Tai/workspace/mapd-data-layer/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src/reason
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/reason/ast.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reason/ast.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/reason/ast.mlast.d : build_deps src/reason/ast.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/reason/ast.cmj $src_root_dir/lib/js/src/reason/ast.js src/reason/ast.cmi : build_cmj_cmi src/reason/ast.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src/reason
build src/reason/parse.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/reason/parse.re
build src/reason/parse.mlast.d : build_deps src/reason/parse.mlast
build src/reason/parse.cmj $src_root_dir/lib/js/src/reason/parse.js src/reason/parse.cmi : build_cmj_cmi src/reason/parse.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src/reason
build build.ninja : phony  ||  src/reason/parse.mlast.d src/reason/ast.mlast.d
